{
  "name": "@wslyhbb/vuejs-datepicker",
  "version": "4.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A simple Vue.js datepicker component. Supports disabling of dates, inline mode, translations. Fork of initial vuejs-datepicker which is abandoned.",
  "keywords": [
    "vue",
    "datepicker",
    "date-picker",
    "calendar"
  ],
  "main": "dist/vuejs-datepicker.common.js",
  "module": "dist/vuejs-datepicker.esm.js",
  "unpkg": "dist/vuejs-datepicker.min.js",
  "style": "dist/vuejs-datepicker.css",
  "files": [
    "src",
    "dist"
  ],
  "author": "Charlie Kassel <ck@charliekassel.com>",
  "contributors": [
    "Simon Tretter <s.tretter@gmail.com>",
    "Wesley Hobbie"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wslyhbb/vuejs-datepicker"
  },
  "bugs": {
    "url": "https://github.com/wslyhbb/vuejs-datepicker/issues"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && npm run build:main:browser && npm run build:main:es && npm run build:main:umd && npm run build:main:cjs",
    "build:main:browser": "rollup --config build/rollup.config.browser.mjs",
    "build:main:es": "rollup --config build/rollup.config.es.mjs",
    "build:main:umd": "rollup --config build/rollup.config.umd.mjs",
    "build:main:cjs": "rollup --config build/rollup.config.cjs.mjs",
    "prepublishOnly": "npm run lint && npm run build",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "serve": "vite --mode loc",
    "test": "jest --config test/unit/jest.conf.js --coverage",
    "lint": "eslint --ext .js,.vue,.ts src test/unit/specs"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "date-fns": "^2.29.3"
  },
  "peerDependencies": {
    "vue": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.6",
    "@vue/vue3-jest": "^29.2.2",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "clean-css": "^5.3.1",
    "coveralls": "^3.0.3",
    "eslint": "^8.30.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.20",
    "pre-commit": "^1.2.2",
    "rollup": "^3.9.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "stylus": "^0.59.0",
    "vite": "^4.0.3",
    "vue": "^3.2.45",
    "vue-eslint-parser": "^9.1.0"
  }
}
